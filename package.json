{
	"name": "reaction-roles",
	"version": "1.1.0",
	"description": "A Discord bot for reaction-based role assignment.",
	"main": "dist/reaction.js",
	"author": "Carter Himmel <fyko@sycer.com>",
	"contributors": [
		"iCrawl",
		"1Computer1"
	],
	"license": "Apache-2.0",
	"scripts": {
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts",
		"cleardir": "rimraf dist/*",
		"compile": "tsc",
		"build": "pnpm run cleardir && pnpm run compile",
		"start": "pnpm run build && node ."
	},
	"dependencies": {
		"common-tags": "^1.8.0",
		"discord-akairo": "github:discord-akairo/discord-akairo",
		"discord.js": "github:discordjs/discord.js",
		"moment": "^2.24.0",
		"moment-duration-format": "^2.3.2",
		"mongoose": "^5.7.4",
		"node-emoji": "^1.10.0",
		"node-superfetch": "^0.1.9",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-angular": "^8.3.4",
		"@types/common-tags": "^1.8.0",
		"@types/dotenv": "^8.2.0",
		"@types/moment-duration-format": "^2.2.2",
		"@types/mongoose": "^5.5.20",
		"@types/node": "^12.7.12",
		"@types/node-emoji": "^1.8.1",
		"@types/ws": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"dotenv": "^8.1.0",
		"eslint": "^6.8.0",
		"eslint-config-marine": "^6.0.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-prettier": "^3.1.2",
		"husky": "^4.2.3",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"tslib": "^1.11.1",
		"typescript": "^3.8.2",
		"winston": "^3.2.1"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.1",
		"erlpack": "discordapp/erlpack",
		"utf-8-validate": "^5.0.2",
		"zlib-sync": "^0.1.6"
	},
	"eslintConfig": {
		"extends": "marine/prettier/node",
		"rules": {
			"comma-dangle": [
				"error",
				"always-multiline"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-angular"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"chore",
					"build",
					"ci",
					"docs",
					"feat",
					"fix",
					"perf",
					"refactor",
					"revert",
					"style",
					"test"
				]
			]
		}
	}
}
